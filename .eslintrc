{
  "extends": ["plugin:prettier/recommended", "prettier/standard", "prettier/react"],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2020,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
    "react/props-validation": "off"
  }
}